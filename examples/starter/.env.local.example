# Cloudflare Workers AI Configuration
# Get these values from: https://dash.cloudflare.com/profile/api-tokens

# Your Cloudflare API Token (Create a custom token with Cloudflare Workers:Edit permissions)
CLOUDFLARE_API_TOKEN=your-cloudflare-api-token-here

# Your Cloudflare Account ID (Found in the right sidebar of your Cloudflare dashboard)
CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id-here

# Optional: Cloudflare Gateway (for custom domain)
# CLOUDFLARE_GATEWAY_ID=your-gateway-id-here

# Optional: Multiple regions for failover
# CLOUDFLARE_REGIONS=[{"name":"us","accountId":"xxx","gatewayId":"yyy"},{"name":"eu","accountId":"xxx","gatewayId":"zzz"}]